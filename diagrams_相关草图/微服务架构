        <mxCell id="z0bP36Qi0vMmJyPEihI5-48" value="通用域" style="rounded=1;whiteSpace=wrap;html=1;fontStyle=1;fontFamily=Comic Sans MS;fontSource=https%3A%2F%2Ffonts.googleapis.com%2Fcss%3Ffamily%3DArchitects%2BDaughter;fontSize=14;sketch=1;" vertex="1" parent="1">
